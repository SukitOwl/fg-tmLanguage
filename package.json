{
    "name": "fg-tmLanguage",
    "displayName": "fg-tmLanguage",
    "description": "Make highlight on .fg file",
    "version": "0.1.0",
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "fg",
            "aliases": ["fg", "fg"],
            "extensions": [".fg"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "fg",
            "scopeName": "source.fg",
            "path": "./syntaxes/fg.tmLanguage"
        }]
    },
    "publisher":"Sukit",
    "repository": "https://github.com/SukitOwl/fg-tmLanguage.git"
}
